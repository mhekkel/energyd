<?xml version="1.0" encoding="UTF-8"?>
<project version="2.0">
  <syspaths>
    <path>../boost/include</path>
    <path>/usr/include</path>
  </syspaths>
  <userpaths>
    <path>.</path>
    <path>src</path>
  </userpaths>
  <libpaths>
    <path>../boost/lib</path>
    <path>/usr/lib</path>
  </libpaths>
  <files>
    <group name="soap">
      <file>exception.cpp</file>
      <file>soap-envelope.cpp</file>
      <file>soap-server.cpp</file>
      <group name="http">
        <file>connection.cpp</file>
        <file>http-server.cpp</file>
        <file>reply.cpp</file>
        <file>request_parser.cpp</file>
      </group>
      <group name="xml">
        <file>document.cpp</file>
        <file>doctype.cpp</file>
        <file>node.cpp</file>
        <file>parser.cpp</file>
        <file>unicode_support.cpp</file>
        <file>writer.cpp</file>
        <file>xpath.cpp</file>
      </group>
    </group>
  </files>
  <targets>
    <target kind="Static Library">
      <name>zeep Debug</name>
      <linkTarget>libzeep</linkTarget>
      <compiler>/usr/bin/c++</compiler>
      <defines>
        <define>DEBUG</define>
      </defines>
      <cflags>
        <cflag>-pthread</cflag>
        <cflag>-gdwarf-2</cflag>
        <cflag>-fPIC</cflag>
        <cflag>-O0</cflag>
      </cflags>
      <warnings>
        <warning>all</warning>
        <warning>no-deprecated</warning>
      </warnings>
    </target>
    <target kind="Static Library">
      <name>zeep Release</name>
      <linkTarget>libzeep</linkTarget>
      <compiler>/usr/bin/c++</compiler>
      <defines>
        <define>NDEBUG</define>
      </defines>
      <cflags>
        <cflag>-pthread</cflag>
        <cflag>-gdwarf-2</cflag>
        <cflag>-fPIC</cflag>
        <cflag>-O3</cflag>
      </cflags>
      <warnings>
        <warning>all</warning>
        <warning>no-deprecated</warning>
      </warnings>
    </target>
  </targets>
</project>
